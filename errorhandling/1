#include<stdio.h>
#include <sys/types.h>
#include <sys/stat.h>
#include <fcntl.h>

int main()
{
        FILE *fp;
        fp=fopen("saroj.txt",O_RDONLY);
        if(fp == -1)
        {
               perror("error only");
        }
        else
        {
                printf("located successfully");
        }


}

